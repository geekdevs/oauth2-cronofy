{
    "name": "geekdevs/oauth2-cronofy",
    "description": "Cronofy OAuth 2.0 Client Provider for The PHP League OAuth2-Client",
    "license": "MIT",
    "authors": [
        {
            "name": "Pavel Dubinin",
            "email": "geekdevs@gmail.com"
        }
    ],
    "keywords": [
        "cronofy",
        "calendar",
        "oauth",
        "oauth2",
        "client",
        "authorization",
        "authentication"
    ],
    "minimum-stability": "beta",
    "require": {
        "php": "^7.4",
        "league/oauth2-client": "^2.5",
        "somoza/oauth2-client-middleware": "~0.3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8.21",
        "mockery/mockery": "^1.3.1",
        "squizlabs/php_codesniffer": "^2.5.1"
    },
    "autoload": {
        "psr-4": {
            "Geekdevs\\OAuth2\\Client\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Geekdevs\\OAuth2\\Client\\Test\\": "tests/src/"
        }
    }
}
